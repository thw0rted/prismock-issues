generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FooCuid {
  id   String  @id @default(cuid())
  name String?
}

model FooUuid {
  id   String  @id @db.Uuid @default(uuid())
  name String?
}
